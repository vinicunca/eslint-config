{
  "name": "@vinicunca/eslint-config",
  "type": "module",
  "version": "2.15.1",
  "description": "Vinicunca ESLint config",
  "author": {
    "name": "praburangki",
    "email": "praburangki@gmail.com",
    "url": "https://praburangki.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinicunca/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/vinicunca/eslint-config/issues"
  },
  "keywords": [
    "JavaScript Linter",
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "enforce",
    "eslint",
    "eslintconfig",
    "eslint-config",
    "hint",
    "jscs",
    "jshint",
    "lint",
    "policy",
    "quality",
    "simple",
    "style",
    "style checker",
    "style linter"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "lint": "eslint -v",
    "test": "vitest",
    "build": "pnpm run typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm --clean",
    "typegen": "tsx scripts/typegen.ts",
    "publish:ci": "pnpm publish --access public --no-git-checks",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.17.3",
    "@prettier/plugin-xml": "^3.4.1",
    "astro-eslint-parser": "^1.1.0",
